@{
  Layout = "_Layout";
}

@model BestRestauraunts.Models.Cuisine

<h2>You have selected: @Model.Style Style!</h2>
<hr>

<ul>
  @foreach(Restauraunt restauraunt in @Model.Restauraunts)
  {
    <li>@restauraunt.Name</li>
  }
</ul>

<p>@Html.ActionLink("Show all Cuisines!", "Index")</p>
<p>@Html.ActionLink("Go Home?", "Index", "Home")</p>